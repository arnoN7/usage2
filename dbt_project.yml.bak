name: 'dep'

config-version: 2
version: '0.0.1'

profile: 'my-snowflake-db'

model-paths: ["models"]
analysis-paths: ["analyses"]
test-paths: ["tests"]
seed-paths: ["seeds"]
macro-paths: ["macros"]
snapshot-paths: ["snapshots"]

docs-paths: ["docs"]
asset-paths: ["assets"]

models:
  +persist_docs:
    relation: true
    columns: true
  dep:
    dwh:
      +materialized: view
      +schema: DWH
      +docs:
         show: true

vars:
  # default number of fake clients to add
  nb_clients: 30
